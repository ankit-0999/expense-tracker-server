# Copy this file to .env and set every value. No defaults are used in code.

# MongoDB connection string (e.g. Atlas or local)
MONGODB_URL=

# Database name
DATABASE_NAME=

# JWT signing secret (use a strong random value in production)
JWT_SECRET=

# JWT algorithm (usually HS256)
JWT_ALGORITHM=

# JWT token expiry in minutes (e.g. 10080 for 7 days)
JWT_EXPIRE_MINUTES=

# Comma-separated CORS origins (e.g. frontend URLs)
CORS_ORIGINS=
